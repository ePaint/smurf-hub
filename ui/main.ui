   <string notr="true">/* Set the background color for all widgets */
QWidget {
    background-color: #0d1819;
}

/* Style for QLabel (adjust fonts and text color) */
QLabel {
    color: #858261;
    font-size: 12px;
}

QLabel::disabled {
    color: #19302e;
}

/* Style for QLabel */
QLabel#accounts_header, QLabel#settings_header {
    color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #30584f, stop: 1 #44605b);
    font-weight: bold; /* Make the font thick */
    font-size: 18px; /* Adjust the font size as needed */
}

/* Style for QLineEdit (adjust border, background, and text color) */
QLineEdit {
    border: 1px solid #858261;
    background-color: #213a38;
    color: #858261;
    padding: 5px;
}

QLineEdit::disabled {
	border: 1px solid #19302e;
    background-color: #0c1718;
}

/* Style for QPushButton (adjust background, text color, and squared borders) */
QPushButton {
    background: qlineargradient(x1: 0, y1: 1, x2: 0, y2: 0, stop: 0 #132326, stop: 0.4 #193233, stop: 1 #254b41);
    color: #858261;
    border: 2px solid #858261; /* Squared border */
    /* border-radius: 0px; */
    font-size: 14px;
    padding: 0px;
}

QPushButton#paypal_button:hover {
    background: #ffffff;
}

/* Style for QPushButton (adjust background, text color, and squared borders) */
QPushButton::disabled {
    background: #213a38;
    color: #19302e;
	border: 1px solid #19302e;
    background-color: #0c1718;
}

/* Change QPushButton appearance on hover and pressed states */
QPushButton::enabled:hover {
    background-color: #0c1718;
}

QPushButton::enabled:pressed {
    background-color: #0c1718;
    color: #858261; /* Restore text color on press */
}

/* Style for QTableWidget (adjust border and row height) */
QTableWidget {
    border: 1px solid #858261;
    alternate-background-color: #213a38;
    color: #858261; /* Text color for table cells */
}

/* Style for table header (adjust fonts, background color, and text color) */
QHeaderView::section {
    background-color: #0c1718;
    color: #858261;
    font-size: 14px;
    font-weight: bold; /* Set the font weight to normal for the header */
}

/* Style for QScrollBar (adjust background, handle, and border colors) */
QScrollBar:vertical {
    background: rgba(11, 20, 16, 128); /* Slightly transparent background */
    border: 2px solid transparent; /* Thicker border for main scroll bar */

    /* Hide the arrow buttons by setting their width to zero */
    QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
        width: 0;
		height: 0;
    }
}

QScrollBar::handle:vertical {
    background: rgba(11, 20, 16, 128); /* Slightly transparent background */
    border: 1px solid #858261; /* Border for thumb */
}

/* Change QPushButton appearance on hover and pressed states */
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: rgba(11, 20, 16, 128); /* Slightly transparent background */
}

QScrollBar::add-line:vertical {
	height: 0px;
}

QScrollBar::sub-line:vertical {
	height: 0px;
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	height: 0px;
}

/* Style for QCheckBox */
QCheckBox {
    spacing: 5px; /* Adjust the spacing between the checkbox and text if needed */
}

/* Style for QCheckBox label text */
QCheckBox::indicator {
    color: #858261; /* Text color for the checkbox label */
    width: 12px; /* Adjust the width for square checkboxes */
    height: 12px; /* Adjust the height for square checkboxes */
	border: 1px solid #858261;
}

QCheckBox::indicator:checked {
	background: qlineargradient(x1: 0.5, y1: 0.5, x2: 0, y2: 0, stop: 0 #858261, stop: 1 #b59e3a);
}
